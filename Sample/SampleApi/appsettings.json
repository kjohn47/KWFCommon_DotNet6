{
  "__comment__": "Add on LogLevel \"Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware\": \"Information\" for HttpLogs",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "LoggingConfiguration": {
    "EnableApiLogs": false,
    "__coment__": "if true, must be added to 'Logging:LogLevel[]' \"Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware\": \"Information\"",
    "EnableHttpLogs": false,
    "Providers": [
      "Console",
      "Debug"
    ]
  },
  "AppConfiguration": {
    "SwaggerSettings": {
      "ApiName": "SampleApi",
      "BearerHeaderKey": "Authorization",
      "ApiVersion": 1
    },
    "KestrelConfiguration": {
      "HttpPort": 4747,
      "HttpsPort": 14747,
      "DisableHttps": null,
      "ListenIpAddresses": null,
      "KestrelCertificateSettings": null
    },
    "LocalizationConfiguration": null,
    "CorsConfiguration": {
      "AllowAnyOrigin": true,
      "AllowAnyHeader": true,
      "AllowAnyMethod": true
    }
  },
  "BearerConfiguration": {
    "TokenIdentifier": "Authorization",
    "TokenKey": "MyTokenKeyNotHashedYet",
    "TokenValidateLife": false
  },
  "KwfCacheConfiguration": {
    "CleanupInterval": {
      "Seconds": 0,
      "Minutes": 15,
      "Hours": 0
    },
    "CacheKeySettings": {
      "WEATHER_SUMARIES": {
        "Expiration": {
          "Seconds": 0,
          "Minutes": 1,
          "Hours": 0
        }
      }
    }
  },
  "KwfRedisCacheConfiguration": {
    "Endpoints": [
      {
        "Url": "localhost",
        "Port": 6147
      },
      {
        "Url": "localhost",
        "Port": 6148
      }
    ],
    "Password": "MyRedisCachePassword",
    "DefaultCacheInterval": {
      "Seconds": 0,
      "Minutes": 30,
      "Hours": 0
    },
    "CachedKeySettings": {
      "WEATHER_SUMARIES": {
        "Expiration": {
          "Seconds": 30,
          "Minutes": 1,
          "Hours": 0
        }
      }
    }
  },
  "AllowedHosts": "*"
}
