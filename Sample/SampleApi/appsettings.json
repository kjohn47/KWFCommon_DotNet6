{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
      /*"Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"*/
    }
  },
  "LoggingConfiguration": {
    "EnableApiLogs": false,
    "EnableHttpLogs": false, /*if true, must be added to 'Logging:LogLevel[]' "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"*/
    "Providers": [
      "Console",
      "Debug"
    ]
  },
  "AppConfiguration": {
    "SwaggerSettings": {
      "ApiName": "SampleApi",
      "BearerHeaderKey": "Authorization",
      "ApiVersion": 1
    },
    "KestrelConfiguration": {
      "HttpPort": 4747,
      "HttpsPort": 14747,
      "ListenIpAddresses": null,
      "KestrelCertificateSettings": null
    },
    "LocalizationConfiguration": null,
    "CorsConfiguration": {
      "AllowAnyOrigin": true,
      "AllowAnyHeader": true,
      "AllowAnyMethod": true
    }
  },
  "BearerConfiguration": {
    "TokenIdentifier": "Authorization",
    "TokenKey": "MyTokenKeyNotHashedYet",
    "TokenValidateLife": false
  },
  "KwfCacheConfiguration": {
    "CleanupInterval": {
      "Seconds": 0,
      "Minutes": 15,
      "Hours": 0
    },
    "CacheKeySettings": {
      "WEATHER_SUMARIES": {
        "Expiration": {
          "Seconds": 0,
          "Minutes": 1,
          "Hours": 0
        }
      }
    }
  },
  "KwfRedisCacheConfiguration": {
    "Endpoints": [
      {
        "Url": "127.0.0.1",
        "Port": 6147
      }
    ],
    "Password": "InsertRedisPw",
    "DefaultCacheInterval": {
      "Seconds": 0,
      "Minutes": 30,
      "Hours": 0
    },
    "CachedKeySettings": {
      "WEATHER_SUMARIES": {
        "Expiration": {
          "Seconds": 0,
          "Minutes": 1,
          "Hours": 0
        }
      }
    }
  },
  "AllowedHosts": "*"
}
