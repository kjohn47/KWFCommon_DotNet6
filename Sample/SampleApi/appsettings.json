{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "LoggingConfiguration": {
    "EnableApiLogs": true,
    "EnableHttpLogs": true,
    "Providers": [
      "Console",
      "Debug"
    ]
  },
  "AppConfiguration": {
    "SwaggerSettings": {
      "ApiName": "SampleApi",
      "BearerHeaderKey": "Authorization",
      "ApiVersion": 1
    },
    "KestrelConfiguration": {
      "HttpPort": 4747,
      "HttpsPort": 14747,
      "ListenIpAddresses": null,
      "KestrelCertificateSettings": null
    },
    "LocalizationConfiguration": null,
    "CorsConfiguration": {
      "AllowAnyOrigin": true,
      "AllowAnyHeader": true,
      "AllowAnyMethod": true
    }
  },
  "BearerConfiguration": {
    "TokenIdentifier": "Authorization",
    "TokenKey": "MyTokenKeyNotHashedYet",
    "TokenValidateLife": false
  },
  "KwfCacheConfiguration": {
    "CleanupInterval": {
      "Seconds": 0,
      "Minutes": 15,
      "Hours": 0
    },
    "CacheKeySettings": {
      "WEATHER_SUMARIES": {
        "Expiration": {
          "Seconds": 0,
          "Minutes": 1,
          "Hours": 0
        }
      }
    }
  },
  "AllowedHosts": "*"
}
