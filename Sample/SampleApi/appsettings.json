{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "LoggingConfiguration": {
    "EnableApiLogs": true,
    "EnableHttpLogs": true,
    "Providers": [
      "Console",
      "Debug"
    ]
  },
  "AppConfiguration": {
    "SwaggerSettings": {
      "ApiName": "SampleApi",
      "BearerHeaderKey": "Authorization",
      "ApiVersion": 1
    },
    "KestrelConfiguration": {
      "HttpPort": 4747,
      "HttpsPort": 14747,
      "ListenIpAddresses": null,
      "KestrelCertificateSettings": null
    },
    "LocalizationConfiguration": null,
    "CorsConfiguration": {
      "AllowAnyOrigin": true,
      "AllowAnyHeader": true,
      "AllowAnyMethod": true
    }
  },
  "BearerConfiguration": {
    "TokenIdentifier": "Authorization",
    "TokenKey": "MyTokenKeyNotHashedYet",
    "TokenValidateLife": false
  },
  "AllowedHosts": "*"
}
